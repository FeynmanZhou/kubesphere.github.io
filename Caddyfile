http://kubesphere.io {
    tls off

    log stdout
    errors stdout
    gzip
    root /srv/kubesphere.github.io/public

    gopkg /kubesphere https://github.com/kubesphere/kubesphere
}

https://www.kubesphere.io, http://www.kubesphere.io {
    redir http://kubesphere.io{uri}
}
